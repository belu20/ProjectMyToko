<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mytoko">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyToko">
        <activity android:name=".ui.home.transaksi.TransaksiActivity"/>
        <activity android:name=".ui.home.home.ProsesScrudActivity" />
        <activity android:name=".ui.home.transaksi.ProsesTransaksiActivity" />
        <activity android:name=".splash.SplashActivity"
            android:theme="@style/TemaSplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_register.RegisterActivity" android:theme="@style/TemaLogin"/>
        <activity android:name=".login_register.LoginActivity" android:theme="@style/TemaLogin"/>
        <activity
            android:name=".ui.home.home.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyToko.NoActionBar"/>
    </application>

</manifest>